<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.synerzip.sqlitedemo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/db2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".NewContactActivity"
            android:theme="@style/AppTheme">
        </activity>

        <activity android:name=".DataListActivity"
            android:theme="@style/AppTheme">
        </activity>

        <activity android:name=".SearchContactActivity"
            android:theme="@style/AppTheme">
        </activity>

        <activity android:name=".UpdateContactActivity"
            android:theme="@style/AppTheme">
        </activity>

    </application>

</manifest>
